<?xml version="1.0" encoding="utf-8"?>
<com.google.android.flexbox.FlexboxLayout xmlns:android="http://schemas.android.com/apk/res/android"
                                          xmlns:app="http://schemas.android.com/apk/res-auto"
                                          android:layout_width="match_parent"
                                          android:layout_height="match_parent"
                                          android:padding="20dp"
                                          app:alignItems="flex_start"
                                          app:flexDirection="row"
                                          app:flexWrap="wrap"
                                          app:justifyContent="flex_start">

    <ImageView
        android:layout_width="100dp"
        android:layout_height="100dp"
        android:layout_margin="5dp"
        android:background="@drawable/xml_drawable_bitmap"/>

    <ImageView
        android:layout_width="80dp"
        android:layout_height="100dp"
        android:layout_margin="5dp"
        android:background="@drawable/xml_drawable_nine_path"/>

    <ImageView
        android:layout_width="100dp"
        android:layout_height="100dp"
        android:layout_margin="5dp"
        android:background="@drawable/xml_drawable_shape"/>

    <ImageView
        android:layout_width="100dp"
        android:layout_height="100dp"
        android:layout_margin="5dp"
        android:background="@drawable/xml_drawable_layer"/>

    <ImageView
        android:layout_width="100dp"
        android:layout_height="100dp"
        android:layout_margin="5dp"
        android:background="@drawable/xml_drawable_state_list"/>

    <ImageView
        android:id="@+id/level_list_image"
        android:layout_width="100dp"
        android:layout_height="100dp"
        android:layout_margin="5dp"
        android:background="@drawable/xml_drawable_level_list"/>

    <ImageView
        android:id="@+id/transition_image"
        android:layout_width="100dp"
        android:layout_height="100dp"
        android:layout_margin="5dp"
        android:background="@drawable/xml_drawable_transition"/>

    <ImageView
        android:layout_width="100dp"
        android:layout_height="100dp"
        android:layout_margin="5dp"
        android:background="@drawable/xml_drawable_inset"/>

    <ImageView
        android:id="@+id/scale_image"
        android:layout_width="100dp"
        android:layout_height="100dp"
        android:layout_margin="5dp"
        android:background="@drawable/xml_drawable_scale"/>

    <ImageView
        android:id="@+id/clip_image"
        android:layout_width="100dp"
        android:layout_height="100dp"
        android:layout_margin="5dp"
        android:background="@drawable/xml_drawable_clip"/>
</com.google.android.flexbox.FlexboxLayout>